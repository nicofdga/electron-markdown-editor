{
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.tsx",
    "src/preload/*.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@renderer/*": [
        "src/renderer/src/*"
      ],
      "@/*": ["./*"]
    }
  }
}
